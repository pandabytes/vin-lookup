// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Vin {
  id Int @id @default(autoincrement())
  vin String @unique // Every @unique column gets an index assigned to it
  make String
  model String
  model_year String
  body_class String
  photo_url String
}
